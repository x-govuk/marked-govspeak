{
  "name": "@x-govuk/marked-govspeak",
  "version": "0.3.0",
  "description": "Govspeak Markdown extensions for Marked.",
  "keywords": [
    "govuk",
    "govspeak",
    "markdown",
    "marked"
  ],
  "homepage": "https://github.com/x-govuk/marked-govspeak#readme",
  "bugs": {
    "url": "https://github.com/x-govuk/marked-govspeak/issues"
  },
  "license": "MIT",
  "files": [
    "lib",
    "govspeak"
  ],
  "main": "index.js",
  "sass": "govspeak/all.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/x-govuk/marked-govspeak.git"
  },
  "scripts": {
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "lint:js": "eslint '**/*.js'",
    "lint:js:fix": "eslint '**/*.js' --fix",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:scss:fix": "stylelint '**/*.scss' --fix",
    "lint": "npm run lint:prettier && npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:prettier:fix && npm run lint:js:fix && npm run lint:scss:fix",
    "test": "node --test",
    "coverage": "node --test --experimental-test-coverage",
    "release": "np"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "marked": "^11.0.0"
  },
  "devDependencies": {
    "@x-govuk/eslint-config": "^0.0.1",
    "np": "^10.0.0",
    "prettier": "^3.1.0",
    "stylelint": "^16.0.0",
    "stylelint-config-gds": "^2.0.0",
    "stylelint-order": "^6.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
