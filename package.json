{
  "name": "@x-govuk/marked-govspeak",
  "version": "0.1.0",
  "description": "Govspeak Markdown extensions for Marked.",
  "keywords": [
    "govuk",
    "govspeak",
    "markdown",
    "marked"
  ],
  "homepage": "https://github.com/x-govuk/marked-govspeak#readme",
  "bugs": {
    "url": "https://github.com/x-govuk/marked-govspeak/issues"
  },
  "license": "MIT",
  "files": [
    "lib",
    "govspeak"
  ],
  "main": "index.js",
  "sass": "govspeak/all.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/x-govuk/marked-govspeak.git"
  },
  "scripts": {
    "lint": "standard && stylelint 'govspeak/**/*.scss'",
    "test": "ava",
    "test:watch": "ava --watch",
    "coverage": "c8 ava"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "marked": "^5.0.5"
  },
  "devDependencies": {
    "ava": "^5.0.0",
    "c8": "^8.0.0",
    "standard": "^17.0.0",
    "stylelint": "^14.0.0",
    "stylelint-config-gds": "^0.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "c8": {
    "reporter": [
      "text",
      "lcovonly"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-gds/scss"
  }
}
